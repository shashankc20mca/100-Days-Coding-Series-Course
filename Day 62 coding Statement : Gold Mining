#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter the number of testcases"<<endl;
    cin>>n;
    int arr[n][3];
    for(int i=0;i<n;i++){
        for(int q=0;q<3;q++){
            cin>>arr[i][q];
        }
    }
     for(int i=0;i<n;i++){
            arr[i][0]=arr[i][0]+1;
     int d;
     d=arr[i][0]*arr[i][2];
     if(d>=arr[i][1]){
        cout<<"Yes"<<endl;
     }
     else{
        cout<<"No"<<endl;
     }


    }



return 0;}
