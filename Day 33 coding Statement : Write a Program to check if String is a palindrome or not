#include<iostream>
#include<string>
using namespace std;
int main(){
string s,r="";
cout<<"Enter the string"<<endl;
getline(cin,s);
for(int i=s.length()-1;i>=0;i--)
{
    r.push_back(s[i]);
}
cout<<r<<endl;
if(r==s){
    cout<<"Palindrome"<<endl;
}
else{
    cout<<"Not a palindrome"<<endl;
}



return 0;
}
