#include<iostream>
using namespace std;
int main(){
    int t;
    cout<<"Enter the number of testcases"<<endl;
    cin>>t;
    int arr[t][2];
    for(int i=0;i<t;i++){
        for(int h=0;h<2;h++){
            cin>>arr[i][h];
        }
    }
    for(int i=0;i<t;i++){
        int r;
        r=arr[i][0]-arr[i][1];
        cout<<r<<endl;
    }





return 0;}
