#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter the number of test cases"<<endl;
    cin>>n;
    int arr[n][2];
    for(int i=0;i<n;i++){
        for(int q=0;q<2;q++){
            cin>>arr[i][q];
        }
    }
      for(int i=0;i<n;i++){
       int g;
       g=arr[i][0]+arr[i][1];
       if(g<3){cout<<"1"<<endl;}
       else if(3<=g&g<=10 ){cout<<"2"<<endl;}
       else if(11<=g&g<=60){cout<<"3"<<endl;}
       else{
        cout<<"4"<<endl;
       }
    }




return 0;}
