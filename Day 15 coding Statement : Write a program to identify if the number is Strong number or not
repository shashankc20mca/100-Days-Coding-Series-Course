#include<iostream>
using namespace std;
int main(){
    int a,r,sum=0,cpy;
    cout<<"Enter the number"<<endl;
    cin>>a;
    cpy=a;
    if(a==0){
        int fact =1;
            for(int i=r;i>0;i--){
            fact=fact*i;
        }

        sum=sum+fact;
    }
    while(a>0){
 int fact=1;
        r=(a%10);
        for(int i=r;i>0;i--){
            fact=fact*i;
        }

    sum=sum+fact;
    a=a/10;
    }


    cout<<sum<<endl;

    if(cpy==sum){
        cout<<"strong number"<<endl;

    }
    else{
        cout<<"Not a strong number"<<endl;
    }
return 0;
}
