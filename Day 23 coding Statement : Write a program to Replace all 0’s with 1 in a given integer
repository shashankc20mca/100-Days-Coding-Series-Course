#include<iostream>
using namespace std;
int main(){
int a,r,s=0,q=0;
cout<<"Enter the number"<<endl;
cin>>a;
if(a==0){
    cout<<1;
}
else{
while(a>0){
    if(a%10==0){
        a=a+1;

    }
    r=a%10;
    s=(s*10)+r;
    a=a/10;

}

while(s>0){
    q=(q*10)+s%10;
    s=s/10;

}
cout<<q<<endl;
}




return 0;
}
