#include<iostream>
using namespace std;
int main(){
    int n,s=0;
    cout<<"Enter the number of elements in the array"<<endl;
    cin>>n;
    int arr[n];
    cout<<"Enter the elements of the array separated  by space"<<endl;
    for(int i=0;i<n;i++){
cin>>arr[i];
s=s+arr[i];
    }
    cout<<s;

return 0;
}

