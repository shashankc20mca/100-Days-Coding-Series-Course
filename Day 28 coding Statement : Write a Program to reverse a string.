#include <iostream>
#include<string>
using namespace std;


int main() {
    // Write C++ code here
    string s;
    int l;
    cout << "enter the string"<<endl;
    cin>>s;
    l=s.length();
    int a=l-1;
    for(int i=0;i<(l/2);i++){
       int cp;

        cp=s[i];
        s[i]=s[a];
        s[a]=cp;
        a=a-1;
    }
    cout<<s;


    return 0;
}
