#include<iostream>
using namespace std;
int main(){
int n;
cout<<"Enter the number of testcases"<<endl;
cin>>n;
int arr[n][2];
for(int i=0;i<n;i++){
        for(int q=0;q<2;q++){
            cin>>arr[i][q];
        }}
 for(int i=0;i<n;i++){
int c;
c=arr[i][1]*arr[i][1];
            c=arr[i][0]/c;
            //cout<<c<<endl;
            if(c<=18){
                cout<<"1"<<endl;
            }
            else if(c>=19&c<=24){
                cout<<"2"<<endl;
            }
            else if (c>=25&c<=29){
                    cout<<"3"<<endl;

            }
            else{
                cout<<"4"<<endl;
            }



}



return 0;}
