#include<iostream>
#include<string>
using namespace std;
int main(){
    string s,n;
    int m=0;

    cout<<"Enter the first string"<<endl;
    cin>>s;
   cout<<"Enter the second string"<<endl;
  cin>>n;

  if(s.length()==n.length()){
   for(int i=0;i<s.length();i++){
    for(int r=0;r<n.length();r++){
        if(s[i]==n[r]){
                m=m+1;
                n[r]=' ';
            break;
        }
    }
   }

   if(m==s.length()){
    cout<<"Anagram"<<endl;
   }
   else{
    cout<<"Not Anagram"<<endl;
   }
   }
   else{
    cout<<"Not Anagram"<<endl;
   }
return 0;
}
