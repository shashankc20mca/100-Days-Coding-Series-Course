#include<iostream>
using namespace std;
int main(){
int n,e=0,o=0;
cout<<"Enter the size of array"<<endl;
cin>>n;
int arr[n];
cout<<"Enter the elements of the array separated by space"<<endl;
for(int i=0;i<n;i++){
cin>>arr[i];
}
for(int i=0;i<n;i++){
    if((arr[i]%2)==0){
            e=e+1;
    }
    else{
        o=o+1;
    }
}
if(e>0&o==0){
    cout<<"even"<<endl;
}
else if(o>0&e==0){
    cout<<"odd"<<endl;
}
else{
    cout<<"mixed"<<endl;
}


return 0;
}
